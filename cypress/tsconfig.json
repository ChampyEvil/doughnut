{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es6",
    "module": "CommonJS",
    "strict": true,
    "allowJs": true,
    "lib": ["es6", "dom"],
    "esModuleInterop": true,
    "typeRoots": ["types", "node_modules/@types"],
    "types": ["cypress", "node", "@testing-library/cypress", "./support"],
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.js"
  ]
}
