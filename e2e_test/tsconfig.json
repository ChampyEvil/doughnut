{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2017",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": "../e2e_test",
    "lib": ["dom", "dom.iterable", "ESNext"],
    "typeRoots": [
      "types",
      "node_modules",
      "node_modules/@types",
      "node_modules/@testing-library",
      "node_modules/@badeball/cypress-cucumber-preprocessor"
    ],
    "paths": {
      "@badeball/cypress-cucumber-preprocessor": [
        "node_modules/@badeball/cypress-cucumber-preprocessor"
      ],
      "@badeball/cypress-cucumber-preprocessor/esbuild": [
        "node_modules/@badeball/cypress-cucumber-preprocessor/dist/subpath-entrypoints/esbuild.d.ts"
      ]
    }
  },
  "exclude": [
    "cypress.config.ts",
    "config/ci.ts",
    "config/common.ts",
    "cypress",
    "node_modules"
  ],
  "include": ["**/*.ts", "**/*.js", "**/*.cjs"]
}
